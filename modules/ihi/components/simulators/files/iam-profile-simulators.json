{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": [
        "s3:ListAllMyBuckets",
        "ecr:GetDownloadUrlForLayer",
        "ecr:BatchGetImage",
        "ecr:DescribeImages",
        "ecr:GetAuthorizationToken",
        "ecr:DescribeRepositories",
        "ecr:ListImages",
        "ecr:BatchCheckLayerAvailability",
        "ecr:GetRepositoryPolicy"
      ],
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": ["s3:GetObject", "s3:ListBucketMultipartUploads", "s3:AbortMultipartUpload"],
      "Resource": [
        "arn:aws:s3:::ihies-pilot-build/*",
        "arn:aws:s3:::ihies-pilot-config/*",
        "arn:aws:s3:::ihies-pilot-build",
        "arn:aws:s3:::ihies-pilot-config"
      ]
    },
    {
      "Sid": "VisualEditor2",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:AbortMultipartUpload",
        "s3:ListBucket",
        "s3:DeleteObject",
        "s3:PutBucketVersioning",
        "s3:GetObjectVersion",
        "s3:ListMultipartUploadParts"
      ],
      "Resource": ["arn:aws:s3:::ihies-pilot-test", "arn:aws:s3:::ihies-pilot-test/*"]
    }
  ]
}
